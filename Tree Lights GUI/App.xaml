<Application x:Class="Tree_Lights_GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Tree_Lights_GUI"
             StartupUri="View\SplashScreenView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <FontFamily x:Key="FontAwesome">/Font/fontawesome-webfont.ttfaFontAwesome</FontFamily>
                </ResourceDictionary>
                <ResourceDictionary Source="Styles\ColoursDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
